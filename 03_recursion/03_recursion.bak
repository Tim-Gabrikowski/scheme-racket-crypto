(define (entf1 o l)
  (cond ((null? l) `())
        ((equal? o (first l)) o)
        (else (entf1 o (rest l)))
  )
)

(define (enth? o l)
  (cond ((null? l) #f)
        ((equal? o (first l)) #t)
        (else (enth? o (rest l)))
  )
)
(define (senth? c s)
  (cond ((null? s) (a))
        (else (enth? c (string->list s)))
  )
)